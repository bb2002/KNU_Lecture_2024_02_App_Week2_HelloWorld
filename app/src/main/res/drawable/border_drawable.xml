    <item>
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="#BE55DA" />
            <solid android:color="#00000000" />
        </shape>
    </item>
    <item
        android:top="3dp"
        android:bottom="3dp"
        android:right="3dp"
        android:left="3dp">
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="#00ff00" />
        </shape>
    </item>
    <item
        android:top="6dp"
        android:bottom="6dp"
        android:right="6dp"
        android:left="6dp">
        <shape android:shape="rectangle">
            <stroke android:width="3dp" android:color="#0000ff" />
        </shape>
    </item>
</layer-list>